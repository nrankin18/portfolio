>\n          <div class=\"company-name\">{this.props.name}</div>\n          <div class=\"company-sub\">{this.props.location}</div>\n          <div>{this.props.detail}</div>\n          <ul class=\"positions\">\n            {this.props.positions.map((position) => {\n              return (\n                <li class=\"position\">\n                  <div class=\"position-name\">{position.name}</div>\n                  <ul class=\"objectives\">\n                    {position.objectives.map((objective) => {\n                      return (\n                        <li\n                          dangerouslySetInnerHTML={{ __html: objective }}\n                        ></li>\n                      );\n                    })}\n                  </ul>\n                </li>\n              );\n            })}\n          </ul>\n        </div>\n      </li>\n    );\n  }\n}\n\nexport default Company;\n","/Users/18nrankin/Documents/Projects/react-portfolio/src/components/Skill.jsx",["95"],"import React from \"react\";\nimport \"../styles.css\";\n\nclass Skill extends React.Component {\n  render() {\n    return (\n      <div class=\"skill\">\n        <img src={this.props.icon} />\n        <div class=\"bubble\">\n          <p>{this.props.name}</p>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default Skill;\n","/Users/18nrankin/Documents/Projects/react-portfolio/src/util/skills.js",[],"/Users/18nrankin/Documents/Projects/react-portfolio/src/util/projects.js",[],"/Users/18nrankin/Documents/Projects/react-portfolio/src/pages/home.jsx",["96","97"],"import React from \"react\";\nimport \"../styles.css\";\nimport Headshot from \"../img/headshot.png\";\nimport Company from \"../components/Company\";\nimport Skill from \"../components/Skill\";\nimport { companies } from \"../util/companies.js\";\nimport { skills } from \"../util/skills.js\";\nimport { projects } from \"../util/projects.js\";\nimport * as Scroll from \"react-scroll\";\n\nimport BCLogo from \"../img/logos/bc.png\";\n